{% extends 'base.html.twig' %}

{% block title %}Hello HomeController!{% endblock %}

{% block body %}
<div class="example-wrapper">
    <ul>
    {% for movie in movies %}
        <li>
            <a href="{{ path('app_movies', {slug: movie.slug}) }}">{{ movie.title }} ({{ movie.genreNames|join(', ') }})</a>
        </li>
    {% endfor %}
    </ul>
</div>
{% endblock %}
